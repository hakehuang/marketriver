<h1>Listing users</h1>

<table>
  <tr>
    <th>Mail</th>
    <th>Nickname</th>
    <th>Phone</th>
    <th>Credits</th>
    <th>Maintenance</th>
    <th>Maintenance Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.mail %></td>
    <td><%= user.nickname %></td>
    <td><%= user.phone %></td>
    <td><%= user.credits %></td>
    <td><%= user.maintenance %></td>
    <td><%= user.Status %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
