
<div id="wrapper">
<div id="header">
</div>
<div id="bodyleft"> 
  <%= render :template => 'leftpop/index' %>
</div>
<div id="bodyright"> 
  <%= render :template => 'rightpop/index' %>
</div>

<div id="bodycenter">

<h2><%= t(:signup) %></h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <p><%= f.label t(:email) %><br />
  <%= f.email_field :email %></p>

  <p><%= f.label t(:passwd) %><br />
  <%= f.password_field :password %></p>

  <p><%= f.label t(:password_confirmation) %><br />
  <%= f.password_field :password_confirmation %></p>

 

  <p><%= f.submit t(:agree) %></p>
<% end %>
  <p> TBD service rules</p>
</div>

