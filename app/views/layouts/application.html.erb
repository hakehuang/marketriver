<!DOCTYPE html>
<html>
<head>
  <title>EnjoyBee</title>
  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="user_nav">
  <% if user_signed_in? %>
    <%= t(:welcome) %> <%= current_user.email %>. <%= t(:notyou)%>
    <%= link_to t(:logout), logout_path, :method => :delete %>
    <%= link_to t(:edit_my_profile_info), customers_path %>
  <% end %>
<%= link_to t(:backtohome),root_path %>
</div>
<%= yield %>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
</body>
</html>
